{
    "name": "laravolt/packer",
    "description": "A cli tool for creating Laravel packages.",
    "keywords": [
        "Packer",
        "Laravel",
        "package",
        "skeleton"
    ],
    "homepage": "https://github.com/laravolt/packer",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra",
            "email": "uyab.exe@gmail.com",
            "homepage": "https://github.com/uyab"
        },
        {
            "name": "Ramon Smit",
            "email": "ramon@daltcore.com",
            "homepage": "https://github.com/ramonsmit"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/filesystem": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Packer\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Packer\\PackerServiceProvider"
            ]
        }
    }
}
